{
  "name": "vsprutor",
  "displayName": "VS Prutor",
  "description": "VSCode Assistant for Prutor",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Education"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "title": "Setup",
      "properties": {
        "vsprutor.server": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "URL of the Prutor server"
        },
        "vsprutor.username": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "Username of student account"
        },
        "vsprutor.password": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "Password of student account"
        }
      }
    },
    "commands": [
      {
        "command": "vsprutor.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "vsprutor.CommitTracker",
        "title": "Hello World CommitTracker"
      },
      {
        "command": "vsprutor.NextWordTracker",
        "title": "Hello World NextWordTracker"
      }
    ],
    "keybindings": [
      {
        "command": "vsprutor.CommitTracker",
        "key": "tab",
        "when": "inlineSuggestionHasIndentationLessThanTabSize && inlineSuggestionVisible && !editorTabMovesFocus"
      },
      {
        "command": "vsprutor.NextWordTracker",
        "key": "ctrl+right",
        "when": "inlineSuggestionVisible && !editorReadonly"
      }
    ],
    "menus": {
      "view/item/context": [
        {
          "command": "vsprutor.helloWorld",
          "group": "inline",
          "when": "viewItem == problemlist"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "vsprutor-panel",
          "title": "VSPrutor",
          "icon": "icon.svg"
        }
      ]
    },
    "views": {
      "vsprutor-panel": [
        {
          "id": "problemlist",
          "name": "Problems"
        },
        {
          "id": "problemStatementView",
          "name": "Problem Statement",
          "type": "webview"
        },
        {
          "id": "testcasesView",
          "name": "Test Cases",
          "type": "webview"
        },
        {
          "id": "executeView",
          "name": "Execute",
          "type": "webview"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "node-fetch": "^2.6.9",
    "node-html-parser": "^6.1.5",
    "sqlite3": "^5.1.4"
  }
}
