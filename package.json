{
  "name": "guardrails",
  "displayName": "GuardRails",
  "description": "Generating guarding rails for your code",
  "publisher": "MrigankPawagi",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrigankpawagi/GuardRails"
  },
  "icon": "icon.png",
  "version": "0.2.6",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Education"
  ],
  "activationEvents": [
    "onView:guardrails.sidebar"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "guardrails.openPanel",
        "title": "Open GuardRails"
      }
    ],
    "keybindings": [
      {
        "command": "guardrails.openPanel",
        "key": "ctrl+shift+g",
        "mac": "cmd+shift+g"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "guardrails-sidebar",
          "title": "GuardRails",
          "icon": "icon.svg"
        }
      ]
    },
    "views": {
      "guardrails-sidebar": [
        {
          "type": "webview",
          "id": "guardrails.sidebar",
          "name": "GuardRails",
          "when": "true"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.75.0",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vscode/test-electron": "^2.2.2",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "node-fetch": "^2.6.9",
    "node-html-parser": "^6.1.5",
    "sqlite3": "^5.1.4"
  }
}
